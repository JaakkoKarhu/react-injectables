{
  "name": "react-injectables",
  "description": "Explicitly inject Elements into any part of your React render tree.",
  "main": "lib/react-injectables.js",
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "NODE_ENV=production $(npm bin)/webpack",
    "commit": "$(npm bin)/git-cz",
    "test": "$(npm bin)/mocha --compilers js:babel-register --recursive --require ./test/setup.js \"test/**/*.test.js\"",
    "lint": "$(npm bin)/eslint src test",
    "semantic-release": "$(npm bin)/semantic-release pre && npm publish && $(npm bin)/semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ctrlplusb/react-injectables.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "injectables",
    "portals",
    "elements"
  ],
  "author": "Sean Matheson <sean@ctrlplusb.com> (http://ctrlplusb.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ctrlplusb/react-injectables/issues"
  },
  "homepage": "https://github.com/ctrlplusb/react-injectables#readme",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-typecheck": "3.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.7.2",
    "chai": "3.5.0",
    "commitizen": "2.7.6",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-mocha": "2.0.0",
    "eslint-plugin-react": "4.2.3",
    "ghooks": "1.1.1",
    "jsdom": "8.3.0",
    "mocha": "2.4.5",
    "path": "0.12.7",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "semantic-release": "6.2.1",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "stats-webpack-plugin": "0.3.1",
    "teaspoon": "6.2.0",
    "webpack": "1.12.14"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-rc.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
